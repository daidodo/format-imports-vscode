{
  "maxBindingNamesPerLine": 0,
  "maxDefaultAndBindingNamesPerLine": 0,
  "maxLineLength": 120,
  "keepUnused": [".*"],
  "sortRules": { "names": ["_", "Aa"] },
  "groupRules": [
    { "flags": "scripts", "regex": "^a", "subGroups": ["^ab", "^aa"] },
    "^a",
    { "flags": "scripts", "regex": "^b", "sort": { "names": ["az", "_"], "paths": ["Aa"] } },
    {
      "regex": "^b",
      "subGroups": ["^bb", {}, "^ba"],
      "sort": { "names": ["az", "_"], "paths": ["Aa"] }
    },
    { "flags": "scripts" },
    {},
    { "flags": "scripts", "subGroups": ["^db", "^da", "^d"] },
    ["^db", "^da", "^d"]
  ]
}
