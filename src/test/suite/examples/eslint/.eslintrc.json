{
  "rules": {
    "import/no-useless-path-segments": [
      "error",
      {
        "noUselessIndex": true
      }
    ],
    "no-duplicate-imports": "warn",
    "sort-imports": [
      "error",
      {
        "ignoreCase": false,
        "allowSeparatedGroups": true,
        "memberSyntaxSortOrder": ["multiple", "single", "none", "all" ]
      }
    ]
  }
}
